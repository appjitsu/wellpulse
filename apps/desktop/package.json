{
  "name": "desktop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "windows": "npx @react-native-community/cli run-windows",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@wellpulse/shared-rn": "workspace:*",
    "react": "19.0.0",
    "react-native": "^0.82.1",
    "react-native-macos": "^0.79.0",
    "react-native-windows": "^0.79.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.7",
    "@react-native/eslint-config": "0.79.7",
    "@react-native/metro-config": "0.79.7",
    "@react-native/typescript-config": "0.79.7",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29.6.3",
    "metro-config": "^0.82.5",
    "prettier": "^3.4.2",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "WellpulseDesktop",
      "namespace": "WellpulseDesktop",
      "template": "old/uwp-cpp-app"
    }
  }
}
